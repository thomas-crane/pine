{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [
        true,
        "single"
      ],
      "interface-name": [
        true,
        "never-prefix"
      ],
      "object-literal-sort-keys": false,
      "member-access": [
        true,
        "no-public"
      ],
      "no-shadowed-variable": false
    },
    "rulesDirectory": []
}
